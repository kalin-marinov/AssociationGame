<Window x:Class="AssociationGame.WPF.PlayerCountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssociationGame.WPF"
        mc:Ignorable="d"
        Title="PlayerCount" Height="190.741" Width="275.926">
    <Grid>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="171,90,0,0" VerticalAlignment="Top" Width="77" Height="28" Click="button_Click" />
        <TextBox x:Name="playersTextbox" HorizontalAlignment="Left" Height="28" Margin="15,90,0,0" TextWrapping="Wrap" Text="{Binding Count, Mode=TwoWay}" VerticalAlignment="Top" Width="151"/>
        <Label x:Name="label1" Content="Enter players count:" Margin="15,18,20,75"/>

    </Grid>
</Window>
